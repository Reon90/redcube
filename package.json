{
  "name": "redcube.js",
  "version": "4.0.0",
  "description": "JS renderer based on GLTF to WebGPU or WebGL backends.",
  "main": "dist/redcube",
  "scripts": {
    "client": "webpack serve",
    "server": "webpack --config webpack.server.js",
    "prod": "rm -rf ./dist && webpack --mode production",
    "lint": "eslint --fix --ext .ts ./src/ || true && prettier --write \"src/**/*.ts\""
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.19.0",
    "@typescript-eslint/parser": "5.19.0",
    "@webgpu/types": "^0.1.13",
    "copy-webpack-plugin": "^7.0.0",
    "eslint": "8.13.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "prettier": "^1.15.3",
    "raw-loader": "4.0.2",
    "source-map-support": "^0.5.12",
    "ts-loader": "^9.2.8",
    "typescript": "^3.9.4",
    "url-loader": "4.1.1",
    "webpack": "5.76.0",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "^4.8.1",
    "webpack-node-externals": "^1.7.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Reon90/redcube.git"
  },
  "keywords": [
    "webgl",
    "webgpu",
    "opengl",
    "gltf",
    "Khronos group",
    "3D"
  ],
  "author": "Artem Kunets",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Reon90/redcube/issues"
  },
  "homepage": "https://github.com/Reon90/redcube",
  "dependencies": {
    "draco3d": "^1.3.6",
    "parse-hdr": "^1.0.0"
  }
}
