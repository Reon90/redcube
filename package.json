{
  "name": "redcube.js",
  "version": "5.0.5",
  "description": "JS renderer based on GLTF to WebGPU or WebGL backends.",
  "main": "dist/redcube",
  "type": "module",
  "scripts": {
    "client": "webpack serve --port 8000",
    "server": "webpack --config webpack.server.js",
    "prod": "rm -rf ./dist && webpack --mode production",
    "lint": "eslint --fix --ext .ts ./src/ || true && prettier --write \"src/**/*.ts\"",
    "test": "playwright test"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@webgpu/types": "^0.1.66",
    "copy-webpack-plugin": "^13.0.1",
    "eslint": "^9.39.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "fs-extra": "^11.3.2",
    "jest": "^30.2.0",
    "pixelmatch": "^7.1.0",
    "playwright": "^1.56.1",
    "pngjs": "^7.0.0",
    "prettier": "^3.6.2",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.4.5",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-node-externals": "^3.0.0"
