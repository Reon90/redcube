{
  "name": "redcube.js",
  "version": "4.0.0",
  "description": "JS renderer based on GLTF to WebGPU or WebGL backends.",
  "main": "dist/redcube",
  "scripts": {
    "client": "webpack serve --port 8000",
    "server": "webpack --config webpack.server.js",
    "prod": "rm -rf ./dist && webpack --mode production",
    "lint": "eslint --fix --ext .ts ./src/ || true && prettier --write \"src/**/*.ts\""
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "@webgpu/types": "^0.1.48",
    "copy-webpack-plugin": "^12.0.2",
    "eslint": "^9.12.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "prettier": "^3.3.3",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.3",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0",
    "webpack-node-externals": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Reon90/redcube.git"
  },
  "keywords": [
    "webgl",
    "webgpu",
    "opengl",
    "gltf",
    "Khronos group",
    "3D"
  ],
  "author": "Artem Kunets",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Reon90/redcube/issues"
  },
  "homepage": "https://github.com/Reon90/redcube",
  "dependencies": {
    "draco3d": "^1.5.7",
    "parse-hdr": "^1.0.0"
  }
}
