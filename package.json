{
  "name": "redcube.js",
  "version": "4.0.0",
  "description": "JS renderer based on GLTF to WebGPU or WebGL backends.",
  "main": "dist/redcube",
  "type": "module",
  "scripts": {
    "client": "webpack serve --port 8000",
    "server": "webpack --config webpack.server.js",
    "prod": "rm -rf ./dist && webpack --mode production",
    "lint": "eslint --fix --ext .ts ./src/ || true && prettier --write \"src/**/*.ts\""
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "@webgpu/types": "^0.1.54",
    "copy-webpack-plugin": "^12.0.2",
    "eslint": "^9.19.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "typescript": "^5.7.3",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.0",
    "webpack-node-externals": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Reon90/redcube.git"
  },
  "keywords": [
    "webgl",
    "webgpu",
    "opengl",
    "gltf",
    "Khronos group",
    "3D"
  ],
  "author": "Artem Kunets",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Reon90/redcube/issues"
  },
  "homepage": "https://github.com/Reon90/redcube",
