{
  "name": "redcube.js",
  "version": "4.0.0",
  "description": "JS renderer based on GLTF to WebGPU or WebGL backends.",
  "main": "dist/redcube",
  "scripts": {
    "client": "webpack serve --port 8000",
    "server": "webpack --config webpack.server.js",
    "prod": "rm -rf ./dist && webpack --mode production",
    "lint": "eslint --fix --ext .ts ./src/ || true && prettier --write \"src/**/*.ts\""
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "7.12.0",
    "@typescript-eslint/parser": "7.12.0",
    "@webgpu/types": "0.1.42",
    "copy-webpack-plugin": "12.0.2",
    "eslint": "8.56.0",
    "eslint-plugin-unused-imports": "3.2.0",
    "prettier": "3.3.1",
    "source-map-support": "^0.5.12",
    "ts-loader": "9.5.1",
    "typescript": "5.4.5",
    "webpack": "5.91.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "5.0.4",
    "webpack-node-externals": "3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Reon90/redcube.git"
  },
  "keywords": [
    "webgl",
    "webgpu",
    "opengl",
    "gltf",
    "Khronos group",
    "3D"
  ],
  "author": "Artem Kunets",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Reon90/redcube/issues"
  },
  "homepage": "https://github.com/Reon90/redcube",
  "dependencies": {
    "draco3d": "^1.3.6",
    "parse-hdr": "^1.0.0"
  }
}
